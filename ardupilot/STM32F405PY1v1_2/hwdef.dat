# hw definition file for processing by chibios_pins.py
# Omnibus F4 PRO with on-board current sensor
# with F405 mcu, mpu6000 imu, bmp280 barometer, 7456 series osd  and sdcard
#qplane:122904 lua:120680

MCU STM32F4xx STM32F405xx

HAL_CHIBIOS_ARCH_F405 1

# board ID for firmware load
APJ_BOARD_ID 1111

# crystal frequency
OSCILLATOR_HZ 12000000

define STM32_ST_USE_TIMER 4
define CH_CFG_ST_RESOLUTION 16

# flash size
FLASH_SIZE_KB 1024
FLASH_RESERVE_START_KB 48 #64

# order of I2C buses
I2C_ORDER I2C1

# order of UARTs
SERIAL_ORDER OTG1 USART1 USART2 USART3 EMPTY EMPTY USART6
#SERIAL_ORDER OTG1 USART1 USART3 UART4 USART6

#adc
PC4 BAT_CURR_SENS ADC1 SCALE(1)
PC5 BAT_VOLT_SENS ADC1 SCALE(1)

#LED
PA13 LED0 OUTPUT LOW GPIO(0) # blue
define HAL_GPIO_A_LED_PIN 0
define HAL_GPIO_LED_ON  1
define HAL_GPIO_LED_OFF 0

# Buzzer
PA14 BUZZER OUTPUT GPIO(80) LOW
define HAL_BUZZER_PIN 80
define HAL_BUZZER_ON 1
define HAL_BUZZER_OFF 0

# GPIOs
PC0  PINIO1 OUTPUT GPIO(81) LOW
#PC1  PINIO2 OUTPUT GPIO(82) LOW

#PWM output
PA0  TIM5_CH1   TIM5  PWM(1)  GPIO(50) BIDIR
PA1  TIM5_CH2   TIM5  PWM(2)  GPIO(51)
PA2  TIM2_CH3   TIM2  PWM(3)  GPIO(52) BIDIR 
PA3  TIM2_CH4   TIM2  PWM(4)  GPIO(53)
PB0  TIM1_CH2N  TIM1  PWM(5)  GPIO(54) NODMA
PB1  TIM1_CH3N  TIM1  PWM(6)  GPIO(55) NODMA
#PB0  TIM3_CH3   TIM3  PWM(5)  GPIO(54)
#PB1  TIM3_CH4   TIM3  PWM(6)  GPIO(55)
PB14 TIM8_CH2N  TIM8  PWM(7)  GPIO(56) NODMA
PB15 TIM8_CH3N  TIM8  PWM(8)  GPIO(57) NODMA
PA15 TIM2_CH1   TIM2  PWM(9)  GPIO(58)
PB3  TIM2_CH2   TIM2  PWM(10) GPIO(59) #button
#PB4  TIM3_CH1   TIM3  PWM(11) GPIO(60) NODMA

PB4  TIM3_CH1 TIM3 RCININT PULLDOWN LOW

#BRD_ALT_CONFIG
# ALT(1)
#PA0 UART4_TX  UART4  NODMA  ALT(1)
#PA1 UART4_RX  UART4  NODMA  ALT(1)

# ALT(1)
PA2 USART2_TX USART2  NODMA ALT(1)
PA3 USART2_RX USART2  NODMA ALT(1)

#DMA_PRIORITY S*
#DMA_NOSHARE  USART3_RX

#UART
PB7 USART1_RX USART1
PB6 USART1_TX USART1

PB10 USART3_TX USART3 NODMA
PB11 USART3_RX USART3 NODMA

PC6 USART6_TX USART6
PC7 USART6_RX USART6

PA11 OTG_FS_DM OTG1
PA12 OTG_FS_DP OTG1
PA9 VBUS INPUT OPENDRAIN

# microSD support
PC8  SDIO_D0  SDIO
PC9  SDIO_D1  SDIO
PC10 SDIO_D2  SDIO
PC11 SDIO_D3  SDIO
PC12 SDIO_CK  SDIO
PD2  SDIO_CMD SDIO

STORAGE_FLASH_PAGE 1
define HAL_STORAGE_SIZE 15360

#SPI1
PA4 IMU_CS CS
PC1 IMU_CS2 CS
PA5 SPI1_SCK SPI1
PA6 SPI1_MISO SPI1
PA7 SPI1_MOSI SPI1

#SPI2
PB12 OSD_CS CS
PC3  SPI2_MOSI SPI2
PC2  SPI2_MISO SPI2
PB13 SPI2_SCK SPI2


# note that this board needs PULLUP on I2C pins
PB8 I2C1_SCL I2C1 PULLUP
PB9 I2C1_SDA I2C1 PULLUP

# SPI Device table
SPIDEV bmi160     SPI1 DEVID1 IMU_CS MODE3 1*MHZ 10*MHZ
SPIDEV bmi160     SPI1 DEVID2 IMU_CS2 MODE3 1*MHZ 10*MHZ

SPIDEV mpu6500    SPI1 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV mpu6500    SPI1 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ

SPIDEV mpu9250    SPI1 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV mpu9250    SPI1 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ

SPIDEV bmp280     SPI1 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV bmp280     SPI1 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ


SPIDEV bmi160     SPI2 DEVID1 IMU_CS MODE3 1*MHZ 10*MHZ
SPIDEV bmi160     SPI2 DEVID2 IMU_CS2 MODE3 1*MHZ 10*MHZ

SPIDEV mpu6500    SPI2 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV mpu6500    SPI2 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ

SPIDEV mpu9250    SPI2 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV mpu9250    SPI2 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ

SPIDEV bmp280     SPI2 DEVID1 IMU_CS MODE3 1*MHZ 8*MHZ
SPIDEV bmp280     SPI2 DEVID2 IMU_CS2 MODE3 1*MHZ 8*MHZ
SPIDEV bmp280     SPI2 DEVID3 OSD_CS MODE3 1*MHZ 8*MHZ

SPIDEV osd        SPI2 DEVID3 OSD_CS MODE0 10*MHZ 10*MHZ

# IMU  BMI160 or MPU6500 or mpu9250
IMU BMI160     SPI:bmi160  #ROTATION_ROLL_180
IMU Invensense SPI:mpu6500 ROTATION_YAW_180
IMU Invensense SPI:mpu9250 ROTATION_YAW_180

